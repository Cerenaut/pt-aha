{
  "seed": 42,

  "image_shape": [1, 3, 16, 16],
  "image_resize_factor": 0.5,

  "pretrain_epochs": 1,
  "pretrain_batch_size": 128,

  "study_steps": 60,
  "study_batch_size": 20,

  "dataset":  "CIFAR",

  "ltm_type": "vc",
