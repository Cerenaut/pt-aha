  },
  "stm": {
    "ps": {
        "inhibition_decay": 0.95,
        "knockout_rate": 0.25,
        "init_scale": 10.0,
        "num_units": 225,
        "sparsity": 10,
        "use_stub": false
    },
    "pc": {
        "shift_range": false
